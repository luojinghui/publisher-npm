{
  "name": "publisher-npm",
  "version": "0.16.0",
  "description": "JavaScript library version management, build automation, and publishing tool for npm packages",
  "main": "index.mjs",
  "scripts": {
    "build": "echo \"build  successfully\"",
    "publish:beta": "node index.mjs run --quickBeta --config ./config/build.config.json",
    "publish:release": "node index.mjs run --config ./config/build.config.json",
    "reverse": "node index.mjs run --reverse --config ./config/build.config.json",
    "test": "node index.mjs run --config ./config/build.config.json"
  },
  "bin": {
    "publisher-npm": "index.mjs"
  },
  "keywords": [
    "npm version",
    "npm publish",
    "publisher npm",
    "build",
    "npm publisher",
    "npm build"
  ],
  "author": "Jinghui Luo",
  "email": "luojinghui424@gmail.com",
  "bugs": {
    "url": "https://github.com/luojinghui/npm-publisher/issues",
    "email": "luojinghui424@gmail.com"
  },
  "repository": "https://github.com/luojinghui/publisher-npm.git",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "inquirer": "9.3.3",
    "semver": "^7.6.3"
  }
}
